<html>
  <head>
    <title>{{ file.frontMatter.title }}</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="stylesheet" href="{{ file.frontMatter.basepath }}/css/all.css" />
    {% for css in file.frontMatter.css %}
    <link rel="stylesheet" href="{{ file.frontMatter.basepath }}/css/{{ css }}" />
    {% endfor %}
    <link rel="stylesheet" href="{{ file.frontMatter.basepath }}/css/tether.css" />
    <link rel="stylesheet" href="{{ file.frontMatter.basepath }}/css/drop-theme-arrows-bounce.css" />
    <link rel="stylesheet" href="{{ file.frontMatter.basepath }}/asset/emoji/css/emoji.css" />
    <link rel="icon" href="{{ file.frontMatter.basepath }}/img/favicon.png" />

    <script>window.config = {{ configJSON | safe }}</script>
    <script>window.BASEPATH = '{{ file.frontMatter.basepath }}'</script>
    <script src="{{ file.frontMatter.basepath }}/js/common.js"></script>
    <script src="{{ file.frontMatter.basepath }}/infrastructure.js"></script>
    {% for js in file.frontMatter.js %}
    <script src="{{ file.frontMatter.basepath }}/js/{{ js }}"></script>
    {% endfor %}
    <script>
      $(function () {
        setTimeout(function () {
          $('#main').trigger('scene-start');
        }, 50);
      });
    </script>
  </head>
  <body class="{{ file.frontMatter.body }}">
    {{ contents }}
  </body>
</html>
